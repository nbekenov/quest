{
  "name": "quest",
  "version": "1.0.0",
  "description": "Rearc quest",
  "main": "src/000.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "build": "npm run lint",
    "start": "node src/000.js",
    "test": "npx jest --passWithNoTests --all"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "keywords": [],
  "author": "Rearc",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
